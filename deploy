#!/usr/bin/env bash
set -aeuo pipefail

rsync -avzP \
  --exclude "uploads" \
  --exclude "cache" \
  --exclude "td_cache" \
  --exclude "debug.log" \
  --exclude "plugins" \
  --exclude "advanced-cache.php" \
  --exclude "object-cache.php" \
  --exclude "wp-cache-config.php" \
  src/wp-content/ \
  root@yaoin.net:/var/www/yaoin.net/wp-content/
